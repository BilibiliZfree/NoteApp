<UserControl x:Class="NoteApp.Modules.ModuleName.Views.ViewD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NoteApp.Modules.ModuleName.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel>
            <TextBlock FontSize="20" Text="{Binding MyProperty}" Margin="20" TextWrapping="Wrap"/>
            <TextBlock FontSize="20" Text="{Binding YouProperty}" Margin="20" TextWrapping="Wrap"/>
        </StackPanel>
    </Grid>
</UserControl>
